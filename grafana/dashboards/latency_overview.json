{
  "title": "Latency Overview",
  "schemaVersion": 36,
  "version": 1,
  "panels": [
    {
      "type": "graph",
      "title": "Ingest Message Latency (ms)",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(ingest_message_latency_ms_bucket[5m])) by (le))" }
      ],
      "id": 1
    },
    {
      "type": "stat",
      "title": "Messages (total)",
      "targets": [{ "expr": "sum(ingest_messages_total)" }],
      "id": 2
    },
    {
      "type": "stat",
      "title": "Errors (total)",
      "targets": [{ "expr": "sum(ingest_errors_total)" }],
      "id": 3
    },
    {
      "type": "stat",
      "title": "Reconnects (total)",
      "targets": [{ "expr": "sum(ingest_reconnects_total)" }],
      "id": 4
    }
  ],
  "time": { "from": "now-6h", "to": "now" }
}
